<?xml version="1.0"?>
<appSettings>
    <add key="MessagingSystemType" value="PowerAlley"/>
    <add key="WebMmRootUrl" value="http://localhost/webmm"/>
    <!-- MS for messaging system-->
    <add key="SmsMsUrl" value="http://127.0.0.1:{{mm_http_port}}"/>
    <add key="BasicRequestPath" value="mm/BasicRequest.aspx"/>
    <!-- MUST contain local url to mmwl-->
    <add key="DbType" value="MySQL"/>
    <add key="WEBMMDb" value="Server={{mysql_server}};Database=webmm;Uid={{mysql_username}};Pwd={{mysql_password}};Old Guids=true;charset=utf8;Treat Tiny As Boolean=true;Keepalive=600"/>
    <add key="PhoneBookDb" value="Server={{mysql_server}};Database=phonebook;Uid={{mysql_username}};Pwd={{mysql_password}};Old Guids=true;charset=utf8;Treat Tiny As Boolean=true;Keepalive=600"/>
    <add key="LocalizationDb" value="Server={{mysql_server}};Database=localization;Uid={{mysql_username}};Pwd={{mysql_password}};Old Guids=true;charset=utf8;Treat Tiny As Boolean=true;Keepalive=600"/>
    <!-- end of MSSQL configuration section -->
    <add key="MmsCleanerTimerInterval" value="20"/>
    <add key="OldRecipientsCleanerTimerInterval" value="1440"/>
    <add key="BackendTimerInterval" value="3"/>
    <add key="AdZoneLocationsUpdateTimerInterval" value="1"/>
    <!--in minutes,  0-Disabled,  -->
    <add key="MSPublicURL" value="http://10.10.0.152:8083"></add>
    <!-- MUST contain external url of MMWL WITHOUT last slash-->
    <add key="MMSFTP" value="false"/>
    <add key="SFTPMode" value="false"/>
    <add key="ForceSSL" value="false"/>
    <add key="FileStorage-local" value="FileStorage-local" />
    <add key="FileStorage-synced" value="FileStorage" />
    <add key="RequestEncoding" value="windows-1256"/>
    <!--Encoding for POST request header, Default - windows-1256. -->
    <add key="SmtpServer" value="10.239.68.11"/>
    <add key="SmtpServerUserName" value=""/>
    <add key="SmtpServerPassword" value=""/>
    <add key="SmtpServerPort" value="25"/>
    <add key="SmtpServerUseSSL" value="False"/>
    <add key="SmtpServer2" value=""/>
    <add key="SmtpServer2UserName" value=""/>
    <add key="SmtpServer2Password" value=""/>
    <add key="SmtpServer2Port" value="25"/>
    <add key="SmtpServer2UseSSL" value="False"/>
    <add key="DebugLogDetalizationLevel" value="Info"/><!--Possible values: Verbose, Info, Warning, Error-->
    <!--HTTP timeout when requesting mmwl. DEFAULT = 100 000 ms-->
    <add key="MSRequestTimeout" value="600000"/>
</appSettings>
